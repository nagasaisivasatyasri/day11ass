<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	
	<class name="com.src.model.Product" table="p3" >
	
		<id name="productId" type="integer">
			<column name="productId" />
			<generator class="increment" ><param name="property">c</param></generator>
		</id>
		
		<property name="productName" type="string">
		<column name="productName" length="20"  />
		</property>
	
		
		<property name="productPrice" type="long">
		<column name="productPrice" length="20"  />
		</property>
		
			
		<many-to-one name="c"></many-to-one>
			</class>
</hibernate-mapping>

